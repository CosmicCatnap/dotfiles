! URXVT settings ------------------------------------------------------------
!URxvt.font:xft:Inconsolata:pixelsize=14,xft:Droid Sans Mono:pixelsize=14,xft:DejaVu Sans Mono:pixelsize=14,xft:Droid Sans Fallback:pixelsize=14,xft:FreeSans:pixelsize=14,xft:FreeSans:pixelsize=14
!URxvt.boldFont:xft:Inconsolata:pixelsize=14,xft:Droid Sans Mono:pixelsize=14,xft:DejaVu Sans Mono:pixelsize=14,xft:Droid Sans Fallback:pixelsize=14,xft:FreeSans:pixelsize=14,xft:FreeSans:pixelsize=14
!URxvt.italicFont:xft:Inconsolata:pixelsize=14,xft:Droid Sans Mono:pixelsize=14,xft:DejaVu Sans Mono:pixelsize=14,xft:Droid Sans Fallback:pixelsize=14,xft:FreeSans:pixelsize=14,xft:FreeSans:pixelsize=14
!URxvt.boldItalicFont:xft:Inconsolata:pixelsize=14,xft:Droid Sans Mono:pixelsize=14,xft:DejaVu Sans Mono:pixelsize=14,xft:Droid Sans Fallback:pixelsize=14,xft:FreeSans:pixelsize=14,xft:FreeSans:pixelsize=14

!Droid Sans Mono
!URxvt.font: xft:Droid Sans Mono-10
!URxvt.letterSpace: -1

!DejaVu Sans
URxvt.font: xft:DejaVu Sans Mono-11
URxvt.boldFont: xft:DejaVu Sans Mono-12
URxvt.letterSpace: -1

!Inconsolata
!URxvt.font: xft:Inconsolata-12
!URxvt.boldFont: xft:Inconsolata-12
!URxvt.letterSpace: -1

URxvt.scrollBar:    false
URxvt*urlLauncher:  firefox
URxvt.cursorBlink:  true
URxvt.cursorColor:  #b1eae1
URxvt.shading:      30
URxvt.tintcolor:    #699c93
URxvt.blurradius:   5
URxvt.depth:        32
URxvt*background:   rgba:0000/0000/0200/c800
URxvt*saveLines:    65535
URxvt*termName:     rxvt
URxvt.iso14755:     False


! Extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
URxvt.urlLauncher: xdg-open
URxvt.urlButton: 1
URxvt.underlineURLs: True

! Keybindings
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

! Xft settings
Xft.autohint:               0
Xft.lcdfilter:              lcddefault
! Xft.hintstyle:              hintfull
Xft.hintstyle:              hintslight
Xft.hinting:                1
Xft.antialias:              1
Xft.rgba:                   rgb
Xft.dpi:                    96

! Misc settings
Xcursor.theme:              dmz-aa


